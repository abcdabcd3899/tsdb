          "hide": false,
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH org(t, h, d, m) AS (\n    SELECT\n      $__timeGroupAlias(ts, $__interval),\n      host,\n      device, -- diskio has device name\n      metrics\n    FROM local.system\n    WHERE $__timeFilter(ts)\n      AND category = 'diskio'\n),\nper_tag_per_t(t, h, d, rb, wb) AS (\n    SELECT\n      t, h, d,\n      min(m->>'read_bytes')::bigint,\n      min(m->>'write_bytes')::bigint\n    FROM org\n    GROUP BY t, h, d\n),\ndelta(t, dt, drb, dwb, mint) AS (\n    SELECT t,\n      t - lag(t, 1) OVER (PARTITION BY h, d ORDER BY t) AS dt,\n      GREATEST(rb - lag(rb, 1) OVER (PARTITION BY h, d ORDER BY t), 0) AS drb,\n      GREATEST(wb - lag(wb, 1) OVER (PARTITION BY h, d ORDER BY t), 0) AS dwb,\n      first_value(t) OVER (PARTITION BY h, d ORDER BY t) AS mint\n    FROM per_tag_per_t\n),\nfilter(t, dt, drb, dwb) AS (\n    SELECT t, dt, drb, dwb\n    FROM delta\n    WHERE dt > 0\n      AND t <> mint\n)\nSELECT t AS \"time\"\n  , sum(dwb/dt)::bigint AS \"Write\"\nFROM filter\nWHERE t <> (SELECT max(t) FROM filter)\nGROUP BY t\nORDER BY t",
